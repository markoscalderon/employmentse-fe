<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title><%= title %></title>

    <!-- Bootstrap core CSS -->
    <link href="/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="/stylesheets/style.css" rel="stylesheet">

    <script src="http://d3js.org/d3.v3.min.js"></script>
    <script src="http://d3js.org/topojson.v0.min.js"></script>
  </head>

  <body ng-app="myapp">

    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">EmploymentSE</a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li><a href="#">Dashboard</a></li>
            <li><a href="#">Settings</a></li>
            <li><a href="#">Help</a></li>
          </ul>
        </div>
      </div>
    </nav>

    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-2 sidebar">
          <div><strong>Time Frame</strong></div>
          <div class="timeframe1">
            <div class="checkbox"><label><input type="checkbox" value="2012Q1" />2012 Q1</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2012Q2" />2012 Q2</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2012Q3" />2012 Q3</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2012Q4" />2012 Q4</label></div>
          </div>
          <div class="timeframe2">
            <div class="checkbox"><label><input type="checkbox" value="2013Q1" />2013 Q1</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2013Q2" />2013 Q2</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2013Q3" />2013 Q3</label></div>
            <div class="checkbox"><label><input type="checkbox" value="2013Q4" />2013 Q4</label></div>
          </div>
          <button type="button" class="btn btn-primary btn-lg" onclick="mapit()">Map it!</button>
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main" id="map">
        </div>
      </div>
    </div>

    <script src="/javascripts/script.js"></script>
  </body>
</html>